# CodeClashers Backend Environment Variables
# Copy this file to .env and fill in your secure passwords

# MinIO (S3-compatible storage)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=your_secure_password_min_8_chars

# Redis
REDIS_PASSWORD=your_secure_redis_password

# Judge0 Database
JUDGE0_POSTGRES_DB=judge0
JUDGE0_POSTGRES_USER=judge0
JUDGE0_POSTGRES_PASSWORD=your_secure_judge0_password

# Colyseus
NODE_ENV=production

