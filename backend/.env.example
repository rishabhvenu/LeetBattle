# LeetBattle Backend Environment Variables
# Copy this file to .env and fill in your own secure values

# MinIO (S3-compatible storage)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=your_secure_password_min_8_chars

# Redis (matchmaking queue + caching)
REDIS_PASSWORD=your_redis_password

# Judge0 Database
JUDGE0_POSTGRES_DB=judge0
JUDGE0_POSTGRES_USER=judge0
JUDGE0_POSTGRES_PASSWORD=your_judge0_db_password

# Colyseus
NODE_ENV=development

# OpenAI (for AI features - REQUIRED for production)
# - Time complexity analysis (validates optimal Big-O)
# - Problem generation (admin panel)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-your-openai-api-key-here

# MongoDB (connection string)
MONGODB_URI=mongodb://codeclashers-mongodb:27017/codeclashers

# Matchmaking (optional - defaults to Medium)
# Options: Easy, Medium, Hard
MATCH_DIFFICULTY=Medium
