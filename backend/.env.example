# LeetBattle Backend Environment Variables
# Copy this file to .env and fill in your own secure values

# MinIO (S3-compatible storage)
# Generate strong random passwords for production
MINIO_ROOT_USER=your_minio_username
MINIO_ROOT_PASSWORD=your_secure_password_min_8_chars

# Redis (matchmaking queue + caching)
# Use a strong random password
REDIS_PASSWORD=your_redis_password

# Judge0 Database
JUDGE0_POSTGRES_DB=judge0
JUDGE0_POSTGRES_USER=judge0
JUDGE0_POSTGRES_PASSWORD=your_judge0_db_password

# Environment
NODE_ENV=development
